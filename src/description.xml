<?xml version='1.0' encoding='UTF-8'?>
<description
        xmlns="http://openoffice.org/extensions/description/2006"
        xmlns:dep="http://openoffice.org/extensions/description/2006"
        xmlns:xlink="http://www.w3.org/1999/xlink">
    <identifier value=""/>

    <version value="0.0.1"/>

    <extension-description>
        <src xlink:href="description/description.en" lang="en"/>
    </extension-description>

    <icon>
        <default xlink:href="icons/libreoffice_calc_icon_181048.png"/>
    </icon>

   <dependencies>
      <OpenOffice.org-minimal-version value="3.4" dep:name="OpenOffice.org 3.4 minimum, or more recent" />
   </dependencies>

    <display-name>
        <name>MyExtension</name>
    </display-name>

    <registration>
        <simple-license accept-by="user" suppress-on-update="true">
            <license-text xlink:href="registration/license.txt" lang="fr"/>
        </simple-license>
    </registration>

    <publisher>
        <name xlink:href="myself@mail.com">MySelf</name>
    </publisher>
</description>
